<td class="cabinet-menu">
    <h5 onclick="window.location = '@Url.Action("Index", "PersonalCabinet")'">Личный кабинет</h5>
    <h5 onclick="window.location = '@Url.Action("Statistics", "PersonalCabinet")'">Статистика</h5>
    <h4>Счета</h4>
    <h5 onclick="window.location = 'http://www.forexinn.ru/register/a/broccoli'">Открыть счет</h5>
        <h5 onclick="window.location = '@Url.Action("ActivateAccount", "PersonalCabinet")'">Активировать счет</h5>
        <h5 onclick="window.location = '@Url.Action("AccountStatus", "PersonalCabinet")'">
            Статус счетов 
            @if (ViewData["AccountNotificationsCount"] != null && ViewData["AccountNotificationsCount"] as int? > 0)
            {
                var count = string.Format("({0})", ViewData["AccountNotificationsCount"]);
                <div class="notifications-count">@count</div>
            }
        </h5>
    <h4>Торговые системы</h4>
        <h5 onclick="window.location = '@Url.Action("TradingSystemOrder", "PersonalCabinet", new { id = 1 })'">Советник 1.0</h5>
        <h5 onclick="window.location = '@Url.Action("TradingSystemOrder", "PersonalCabinet", new { id = 2 })'">Советник 2.0</h5>
        <h5 onclick="window.location = '@Url.Action("TradingSystemOrder", "PersonalCabinet", new { id = 3 })'">Советник 3.0</h5>
        <h5 onclick="window.location = '@Url.Action("OrderStatus", "PersonalCabinet")'">
            Статус заказов
            @if (ViewData["TradingSystemNotificationsCount"] != null && ViewData["TradingSystemNotificationsCount"] as int? > 0)
            {
                var count = string.Format("({0})", ViewData["TradingSystemNotificationsCount"]);
                <div class="notifications-count">@count</div>
            }
        </h5>
    <h4>Служба поддержки</h4>
        <h5>Мои письма</h5>
        <h5>Написать</h5>
        <h5>Рекомендации</h5>
</td>